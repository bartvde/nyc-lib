{
  "name": "@timkeane/nyc-lib",
  "version": "0.6.0",
  "description": "A set of common libraries used to develop NYC mapping applications",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "yarn clean && yarn test",
    "build": "webpack",
    "postbuild": "yarn replace-hosts && yarn copy-examples",
    "copy-examples": "cp -r ./examples/new ./dist/examples",
    "replace-hosts": "bash -c 'if [ -f replace-hosts.sh ]; then ./replace-hosts.sh; fi'",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "moduleNameMapper": {
      "^nyc(.*)$": "<rootDir>/src/nyc/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timkeane/nyc-lib.git"
  },
  "keywords": [
    "mapping",
    "openlayers",
    "leaflet",
    "nyc",
    "New York"
  ],
  "author": "Tim Keane <t.d.keane@gmail.com> (https://timkeane.github.io/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/timkeane/nyc-lib/issues"
  },
  "homepage": "https://github.com/timkeane/nyc-lib#readme",
  "engines": {
    "node": ">=4.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^22.4.3",
    "jsdoc": "^3.5.5",
    "pre-commit": "^1.2.2",
    "strict-mode": "^1.1.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "@types/ol": "^4.6.1",
    "jquery": "^2.2.4",
    "jquery-csv": "^0.8.9",
    "jquery-mobile": "^1.5.0-alpha.1",
    "ol": "^4.6.5",
    "proj4": "^2.4.4"
  }
}
